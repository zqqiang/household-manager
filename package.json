{
  "name": "household-manager",
  "version": "1.0.0",
  "description": "This repository is a full-stack household market manager framework.   The target is to make household market management quickly and easy.",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-css": "lessc react/themes/adminlte/less/main.less react/themes/adminlte/css/main.css",
    "watch-js": "webpack -w --config webpack.config.js",
    "watch-go": "fresh",
    "dev": "concurrently -k \"npm run watch-js\" \"npm run watch-go\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zqqiang/household-manager.git"
  },
  "author": "zqqiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zqqiang/household-manager/issues"
  },
  "homepage": "https://github.com/zqqiang/household-manager#readme",
  "dependencies": {
    "adminlte": "^2.4.1",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "ionicons": "^3.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "save-dev": "^2.0.0",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3"
  }
}
